{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true, // Optional but recommended
    "forceConsistentCasingInFileNames": true // Optional but recommended
  },
  "include": ["src/**/*"]
}